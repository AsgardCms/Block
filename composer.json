  "type": "asgard-module",
  "license": "MIT",
  "require": {
    "php": ">=5.6.4",
    "composer/installers": "~1.0",
    "asgardcms/core-module": "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "orchestra/testbench": "3.3.*",
    "fzaninotto/faker": "~1.5",
    "phpro/grumphp": "^0.9.1",
    "friendsofphp/php-cs-fixer": "^1.11"
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\Block\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "n.widart@gmail.com"
    }
  ],
  "extra": {
