{
  "name": "asgardcms/block",
  "description": "A module to create small blocks of content to display anywhere on the site.",
  "type": "asgard-module",
  "license": "MIT",
  "require": {
    "php": ">=5.6",
    "composer/installers": "~1.0",
    "asgardcms/core-module": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "orchestra/testbench": "~3.1",
    "fzaninotto/faker": "~1.5"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/nWidart/modules"
    }
  ],
  "autoload-dev": {
    "psr-4": {
      "Modules\\Block\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "n.widart@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
